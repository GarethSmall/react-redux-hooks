{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {"name": "esdoc-standard-plugin"},
    {"name": "esdoc-flow-type-plugin", "option": {"enable": true}},
    {
      "name": "esdoc-publish-markdown-plugin",
      "option": {
        "filename":"../src/README.md"
      }
    }
  ]
}
